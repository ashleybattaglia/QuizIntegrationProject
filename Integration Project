print("The correct answer is", correct_answer)
print()
#example
print("4.5 would turn into 4 if put into the int() function.")
print()
print("4*5=",4*5)
print("4+5=",4+5)
print("int(4.5)=",int(4.5))
print("4-5=",4-5)
print()

