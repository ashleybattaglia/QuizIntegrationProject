print("for x in soda: ")
print("print(x)")
soda = ['Pepsi', 'Mountain Dew', 'Coke', 'Sprite']
for x in soda:
    print(x)
print()

answer = input("Choose your answer: ")
correct_answer = 'vertical'
if answer == "vertical":
    print("Correct")
elif answer == "up and down":
    print("Specify the term")
else:
    print("Incorrect")
