print()

print("A. 2/5 subtracts 2 from 5")
print("B. 2/5 adds 2 and 5")
print("C. 2//5 multiplies 2 by 5")
print("D. 2//5 divides and dumps the value after the decimal")
print()

user_answer = input("Choose your answer: ")
correct_answer = 'D'
print("The correct answer is", correct_answer)
print()
#explanation
print("2/5 is simple division\n2//5 divides and dumps the numbers after the decimal.")
print()
print("2-5=",2-5)
print("2/5=",2/5)
print("2*5=",2*5)
print("2//5=",2//5)
print()

